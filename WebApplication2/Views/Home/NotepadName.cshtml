
@{
    ViewBag.Title = "NotepadName";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="jumbotron">
    <br />
    <table>
        @for (int i = 0; i < ViewBag.myText.Length; i++)
        {
            <tr>
                <td>@Html.ViewBag.myText[i]</td>
            </tr>
        }
    </table>
</div>